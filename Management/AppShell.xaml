<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Management.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Management.Screens"
    Shell.FlyoutBehavior="Disabled"
    Title="Management">

    <ShellContent
        Title="SIGN-IN"
        ContentTemplate="{DataTemplate local:SignIn}"
        Route="SignIn" />

    <ShellContent
    Title="SIGN-UP"
    ContentTemplate="{DataTemplate local:SignUp}"
    Route="SignUp" />
    
    <ShellContent
    Title="HOME"
    ContentTemplate="{DataTemplate local:Home}"
    Route="Home" />

</Shell>
